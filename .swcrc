{
    "$schema": "https://json.schemastore.org/swcrc",
    "sourceMaps": true,
    "minify": false,
    "jsc": {
        "parser": {
          "syntax": "typescript",
          "tsx": false,
          "decorators": true
        },
        "target": "es2021",
        "keepClassNames": true,
        "transform": {
          "legacyDecorator": true,
          "decoratorMetadata": true
        }
    }
}
