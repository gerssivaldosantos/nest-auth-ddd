{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [{
      "include": "../client",
      "outDir": "dist/client"
    }],
    "watchAssets": true,
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": true
  }
}
