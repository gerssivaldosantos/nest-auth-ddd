{
   "name": "generated",
   "version": "1.0.0",
   "description": "Server",
   "main": "index.js",
   "author": "Notoriun <suporte@notoriun.com.br>",
   "license": "UNLICENSED",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "pretypeorm": "ts-node -r tsconfig-paths/register ./src/database/scripts/copy-typeorm-config.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
      "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./typeOrm.config.ts",
      "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ./src/database/migrations/$npm_config_name",
      "typeorm:create-migration": "npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
      "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert",
      "seed:config": "seeds -- config",
      "seed:run": "seeds -- seed",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "vitest run",
      "test:watch": "vitest",
      "test:cov": "vitest run --coverage",
      "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
      "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
      "postinstall": "eslint \"{src,apps,libs,test}/**/*.{ts,vue}\" --fix"
   },
   "dependencies": {
      "@nestjs/common": "^10.1.3",
      "@nestjs/config": "^3.0.0",
      "@nestjs/core": "^10.1.3",
      "@nestjs/jwt": "^10.1.0",
      "@nestjs/passport": "^10.0.0",
      "@nestjs/platform-express": "^10.1.3",
      "@nestjs/serve-static": "^4.0.0",
      "@nestjs/swagger": "^7.1.8",
      "@nestjs/typeorm": "^10.0.0",
      "argon2": "^0.31.0",
      "axios": "^1.4.0",
      "brazilian-class-validator": "^0.1.6",
      "brazilian-values": "^0.13.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "date-fns": "^2.30.0",
      "dotenv": "^16.0.3",
      "pg": "^8.11.2",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.8.1",
      "sqlite3": "^5.1.6",
      "typeorm": "0.3.17",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@faker-js/faker": "^7.6.0",
      "@nestjs/cli": "^10.1.11",
      "@nestjs/schematics": "^10.0.2",
      "@nestjs/testing": "^10.1.3",
      "@swc/cli": "^0.1.62",
      "@swc/core": "^1.3.76",
      "@types/express": "^4.17.17",
      "@types/jest": "^29.5.3",
      "@types/node": "^18.17.5",
      "@types/supertest": "^2.0.12",
      "@types/uuid": "^9.0.2",
      "@typescript-eslint/eslint-plugin": "^5.59.2",
      "@typescript-eslint/parser": "^5.59.2",
      "@vitest/coverage-v8": "^0.34.1",
      "eslint": "^8.26.0",
      "eslint-config-prettier": "8.5.0",
      "eslint-config-prettier-standard": "4.0.1",
      "eslint-config-standard": "17.0.0",
      "eslint-plugin-import": "2.26.0",
      "eslint-plugin-n": "^15.3.0",
      "eslint-plugin-node": "11.1.0",
      "eslint-plugin-prettier": "4.2.1",
      "eslint-plugin-promise": "6.1.1",
      "faker": "^6.6.6",
      "husky": "^8.0.3",
      "jest": "^29.6.2",
      "prettier": "^2.8.8",
      "prettier-config-standard": "5.0.0",
      "prettier-standard": "16.4.1",
      "source-map-support": "^0.5.21",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.1",
      "ts-loader": "^9.4.4",
      "ts-node": "10.9.1",
      "tsconfig-paths": "^4.2.0",
      "typescript": "4.9.5",
      "unplugin-swc": "^1.3.2",
      "vitest": "^0.34.1",
      "vitest-tsconfig-paths": "^3.4.1"
   },
   "engines": {
      "node": ">=16.0.0"
   }
}
