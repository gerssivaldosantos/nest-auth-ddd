{
   "name": "generated",
   "version": "1.0.0",
   "description": "Server",
   "main": "index.js",
   "author": "Notoriun <suporte@notoriun.com.br>",
   "license": "UNLICENSED",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "pretypeorm": "ts-node -r tsconfig-paths/register ./src/database/scripts/copy-typeorm-config.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
      "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./typeOrm.config.ts",
      "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ./src/database/migrations/$npm_config_name",
      "typeorm:create-migration": "npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
      "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert",
      "seed:config": "seeds -- config",
      "seed:run": "seeds -- seed",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "postinstall": "eslint \"{src,apps,libs,test}/**/*.{ts,vue}\" --fix"
   },
   "dependencies": {
      "@nestjs/passport": "^9.0.3",
      "@nestjs/common": "^9.4.0",
      "@nestjs/config": "^2.3.1",
      "@nestjs/core": "^9.4.0",
      "@nestjs/jwt": "^10.0.3",
      "@nestjs/platform-express": "^9.4.0",
      "@nestjs/serve-static": "^3.0.1",
      "@nestjs/swagger": "^6.3.0",
      "@nestjs/typeorm": "^9.0.1",
      "argon2": "^0.30.3",
      "axios": "^1.4.0",
      "brazilian-class-validator": "^0.1.6",
      "brazilian-values": "^0.12.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "date-fns": "^2.30.0",
      "dotenv": "^16.0.3",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.1",
      "pg": "^8.10.0",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.8.1",
      "sqlite3": "^5.1.6",
      "typeorm": "0.3.15",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@faker-js/faker": "^7.6.0",
      "@nestjs/cli": "^9.1.5",
      "@nestjs/schematics": "^9.1.0",
      "@nestjs/testing": "^9.4.0",
      "@swc/core": "^1.3.56",
      "@swc/jest": "^0.2.26",
      "@types/express": "^4.17.17",
      "@types/jest": "^29.5.1",
      "@types/node": "^16.18.25",
      "@types/passport-jwt": "^3.0.9",
      "@types/supertest": "^2.0.12",
      "@types/uuid": "^9.0.2",
      "@typescript-eslint/eslint-plugin": "^5.59.2",
      "@typescript-eslint/parser": "^5.59.2",
      "eslint": "^8.26.0",
      "eslint-config-prettier": "8.5.0",
      "eslint-config-prettier-standard": "4.0.1",
      "eslint-config-standard": "17.0.0",
      "eslint-plugin-import": "2.26.0",
      "eslint-plugin-n": "^15.3.0",
      "eslint-plugin-node": "11.1.0",
      "eslint-plugin-prettier": "4.2.1",
      "eslint-plugin-promise": "6.1.1",
      "faker": "^6.6.6",
      "husky": "^8.0.3",
      "jest": "^29.5.0",
      "prettier": "^2.8.8",
      "prettier-config-standard": "5.0.0",
      "prettier-standard": "16.4.1",
      "source-map-support": "^0.5.21",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.0",
      "ts-loader": "^9.4.2",
      "ts-node": "10.9.1",
      "tsconfig-paths": "^4.2.0",
      "typescript": "4.9.5"
   },
   "engines": {
      "node": ">=16.0.0"
   }
}
